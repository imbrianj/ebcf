{{#ui-dropdown
  fullTextSearch=true
  onChange=(action onChange)
  selected=selectedTag
  class=(if disabled "disabled search selection tag-search" "search selection tag-search")
  as |execute mapper|}}
  <div class="default text">Seach by Tag</div>
  <i class="dropdown icon"></i>
  <div class="menu">
    {{#each tags as |tag|}}
      <div class="item" data-value="{{map-value mapper tag}}">
        {{tag.value}}
      </div>
    {{/each}}
  </div>
{{/ui-dropdown}}
