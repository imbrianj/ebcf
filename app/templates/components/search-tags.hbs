{{#ui-dropdown class="search selection tag-search" onChange=(action 'tagsEntered')}}
  <div class="default text">Seach by Tag</div>
  <i class="dropdown icon"></i>
  <div class="menu">
    {{#each tags as |tag|}}
      <div class="item" data-value={{tag.id}}>
        {{tag.value}}
      </div>
    {{/each}}
  </div>
{{/ui-dropdown}}
