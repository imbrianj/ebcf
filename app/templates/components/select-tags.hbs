{{#ui-dropdown class="fluid multiple search selection onAdd=(action 'update_selected')"}}

  <div class="default text">Select a tag</div>
  <i class="dropdown icon"></i>
  <div class="menu">
  {{#each tags as |tag|}}
    <div class="item" data-value={{tag.id}}>
      {{tag.value}}
    </div>
  {{/each}}
  </div>
{{/ui-dropdown}}
